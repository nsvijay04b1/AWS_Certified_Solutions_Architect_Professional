{
		"Version": "2012-10-17",
		"Statement": [{
			"Effect": "Allow",
			"Action": [
				"s3:GetObjectAcl",
				"s3:PutObjectAcl"
			],
			"Resource": "arn:aws:s3:::secretcatpics/*"
		}, {
			"Effect": "Allow",
			"Action": [
				"logs:CreateLogGroup",
				"logs:CreateLogStream",
				"logs:PutLogEvents"
			],
			"Resource": "*"
		}]
}